FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistry           on;

chemistryType 
{
    solver     ode;
}

initialChemicalTimeStep 1e-07;

odeCoeffs
{
    solver Rosenbrock34;
    absTol 1e-8; 
    relTol 1e-5; 
}



